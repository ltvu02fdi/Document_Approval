        <!-- Purchase Request -->
        <record id="seq_purchase_request" model="ir.sequence">
            <field name="name">Purchase Request</field>
            <field name="code">purchase.request.seq</field>
            <field name="prefix">DNMH</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Payment Request -->
        <record id="seq_payment_request" model="ir.sequence">
            <field name="name">Payment Request</field>
            <field name="code">payment.request.seq</field>
            <field name="prefix">DNTT</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Cash Request -->
        <record id="seq_cash_request" model="ir.sequence">
            <field name="name">Cash Advance Request</field>
            <field name="code">cash.request.seq</field>
            <field name="prefix">DNTU</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- advance settlement -->
        <record id="seq_advance_settlement" model="ir.sequence">
            <field name="name">Cash Advance Settlement</field>
            <field name="code">advance.settlement.seq</field>
            <field name="prefix">DNQTTU</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Client Request -->
        <record id="seq_client_request" model="ir.sequence">
            <field name="name">Client Request</field>
            <field name="code">client.request.seq</field>
            <field name="prefix">DXTK</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
